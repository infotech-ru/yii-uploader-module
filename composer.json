{
    "name": "terrasoff/yii-uploader-module",
    "description": "File uploader module for Yii Framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Tyutin",
            "email": "anton@tyutin.ru"
        },
        {
            "name": "Tarasov Konstantin",
            "email": "terrasoff@terrasoff.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii": "dev-master",
        "infotech/yii-file-storage": "dev-master",
        "components/jQuery-Ajax-Upload": "dev-master",
        "RobLoach/component-installer": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "components/jQuery-Ajax-Upload",
                "type": "component",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/codler/jQuery-Ajax-Upload",
                    "reference": "v2.0.1",
                    "type": "git"
                },
                "extra": {
                    "name": "codler/jQuery-Ajax-Upload",
                    "component": {
                        "files": [
                            "./jquery.ajaxupload.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ],
    "config": {
        "component-dir": "assets"
    },
    "autoload": {
        "psr-4": {
            "YiiFileUploader\\": "src/",
            "YiiFileUploader\\Controller": "src/Controller"
        }
    }
}
